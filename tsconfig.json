{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    "paths": {
      "@ts/*": ["resources/ts/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["resources/ts/*", "resources/ts/**/*.ts", "resources/ts/**/*.tsx", "resources/ts/Layouts/index.tss", "resources/ts/Components/dashboard"],
  "exclude": ["node_modules"]
}
